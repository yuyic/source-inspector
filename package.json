{
  "name": "source-inspector",
  "version": "0.2.0",
  "license": "MIT",
  "description": "Inspect html elements' source code in IDE",
  "author": "Yichong Yu <yuyicman@gmail.com> (https://github.com/yuyic)",
  "files": [
    "inspector/index.js",
    "inspector/dist",
    "webpack",
    "vite",
    "ast",
    "index.js",
    "launch.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyic/source-inspector"
  },
  "scripts": {
    "inspect": "vite",
    "build": "vite build",
    "test": "run-s -c test:pre \"test:exec {@}\" test:post --",
    "test:webpack-4": "cross-env WEBPACK_VERSION=4 yarn test",
    "test:pre": "run-s yalc:publish yalc:add",
    "test:exec": "cross-env DEBUG=false jest",
    "test:post": "yarn yalc:clean",
    "yalc:add": "yalc add --dev source-inspector",
    "yalc:clean": "yalc remove --all",
    "yalc:publish": "yalc publish --no-scripts",
    "types:clean": "bash ./clean_types.sh",
    "types:compile": "tsc -p tsconfig.json",
    "generate-types": "run-s types:clean types:compile",
    "prepublishOnly": "yarn generate-types"
  },
  "engines": {
    "node": ">= 14.18.1",
    "yarn": "^1.22.4"
  },
  "dependencies": {
    "@babel/generator": "^7.17.3",
    "@babel/parser": "^7.17.3",
    "@babel/traverse": "^7.17.3",
    "@babel/types": "^7.17.0",
    "@vue/compiler-dom": "^3.2.31",
    "get-port": "5.1.1",
    "launch-editor-middleware": "^2.6.0",
    "loader-utils": "^2.0.2",
    "magic-string": "^0.26.1",
    "schema-utils": "^3.1.0",
    "slash": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-react": "^7.22.5",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "8.1.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "is-electron": "^2.2.1",
    "jest": "^27.5.1",
    "jest-environment-node": "27.5.1",
    "less": "^4.1.2",
    "module-alias": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^13.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semver": "^7.5.1",
    "typescript": "^5.1.6",
    "vite": "^2.8.4",
    "webpack": "^5.87.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "4.2.1",
    "webpack.legacy": "npm:webpack@4.x",
    "yalc": "^1.0.0-pre.53",
    "yarn": "^1.22.17",
    "yn": "4.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
