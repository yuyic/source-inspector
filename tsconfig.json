{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "es2018",
    "skipLibCheck": true
  },
  "include": ["index.js", "launch.js", "./webpack/*", "./vite/*"],
  "exclude": ["node_modules"]
}
